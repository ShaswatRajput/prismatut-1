generator client {
    provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") 
  
}

model User {
    id Int @id @default(autoincrement())
    firstName String
    lastName String
    age Int 
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}